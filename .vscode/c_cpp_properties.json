{
  "env": {
    "myIncludePath": [
      "/usr/include/SDL",
      "${workspaceFolder}/include",
      "${workspaceFolder}/src"
    ],
    "myDefines": [
      "DEBUG"
    ]
  },
  "configurations": [
    {
      "name": "Linux",
      "compilerPath": "/usr/bin/gcc",
      "intelliSenseMode": "linux-gcc-x86",
      "includePath": [
        "${myIncludePath}",
        "/usr/include"
      ],
      "defines": [
        "${myDefines}"
      ],
      "cStandard": "gnu11",
      "cppStandard": "gnu++17",
      "browse": {
        "path": [
          "${myIncludePath}",
          "/usr/include"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
      }
    }
  ]
}